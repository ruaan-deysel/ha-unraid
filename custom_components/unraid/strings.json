{
  "title": "Unraid",
  "config": {
    "step": {
      "user": {
        "title": "Connect to Unraid",
        "description": "Connect to your Unraid server using the GraphQL API. Requires Unraid 7.2.0 or newer.",
        "data": {
          "host": "Host",
          "port": "Port",
          "api_key": "API key"
        },
        "data_description": {
          "host": "IP address (for example, 192.168.1.100) or hostname/domain of your Unraid server.",
          "port": "HTTP port number (default: 80). Secure connections are handled automatically.",
          "api_key": "Generate in Unraid Settings → Management Access → API Keys. Requires ADMIN role."
        }
      },
      "reauth_confirm": {
        "title": "Reauthenticate Unraid",
        "description": "The API key for {host} is no longer valid. Please enter a new API key.",
        "data": {
          "api_key": "API key"
        },
        "data_description": {
          "api_key": "Generate a new API key in Unraid Settings → Management Access → API Keys"
        }
      },
      "reconfigure": {
        "title": "Reconfigure Unraid",
        "description": "Update the connection settings for your Unraid server.",
        "data": {
          "host": "Host",
          "port": "Port",
          "api_key": "API key"
        },
        "data_description": {
          "host": "IP address or hostname of your Unraid server.",
          "port": "HTTP port number (default: 80). Secure connections are handled automatically.",
          "api_key": "API key with ADMIN role permissions."
        }
      }
    },
    "error": {
      "cannot_connect": "Cannot connect. Verify the host and port are correct and reachable.",
      "invalid_auth": "Invalid API key or insufficient permissions. Ensure the key has ADMIN role.",
      "unsupported_version": "Unraid version not supported. Minimum required: Unraid 7.2.0 (API v4.21.0).",
      "unknown": "Unexpected error occurred. Check Home Assistant logs for details.",
      "already_configured": "This Unraid server is already configured.",
      "required": "This field is required.",
      "invalid_hostname": "Invalid hostname or IP address format."
    },
    "abort": {
      "already_configured": "This Unraid server is already configured.",
      "reauth_successful": "Reauthentication successful.",
      "reauth_failed": "Reauthentication failed.",
      "reconfigure_successful": "Reconfiguration successful.",
      "reconfigure_failed": "Reconfiguration failed - entry not found."
    }
  },
  "options": {
    "step": {
      "init": {
        "title": "Unraid UPS Settings",
        "description": "Configure UPS power settings for accurate power monitoring.",
        "data": {
          "ups_capacity_va": "UPS capacity (VA)",
          "ups_nominal_power": "UPS nominal power (W)"
        },
        "data_description": {
          "ups_capacity_va": "Optional. Your UPS VA rating (e.g., 1000 for a 1000VA UPS). Shown in sensor attributes for reference.",
          "ups_nominal_power": "Required for UPS Power sensor. Enter the Watts value from your UPS specs. If Unraid only shows VA, check your UPS label or estimate using VA × 0.6."
        }
      }
    },
    "abort": {
      "no_options_available": "No configurable options available. Options are shown when a UPS device is detected."
    }
  },
  "issues": {
    "auth_failed": {
      "title": "Authentication Failed",
      "fix_flow": {
        "step": {
          "confirm": {
            "title": "Reauthenticate Unraid",
            "description": "The API key for {host} is no longer valid. Click submit to enter a new API key."
          }
        }
      }
    }
  },
